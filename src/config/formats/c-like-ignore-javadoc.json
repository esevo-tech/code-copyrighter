{
  "headerRegex": "/^\\/\\*{1}\\s+(.*?)\\s*\\*\//gs",
  "headerContentRegex": "/^[ \\t]*\\*{0,1}[ \\t]*(.*)$/gm",
  "writer": {
    "headerContentTransform": {
      "search": "/.*/",
      "replace": " * $&"
    },
    "headerTransform": {
      "search": "/.*/",
      "replace": "/*\n$&\n */"
    }
  }
